function out = derivative_novel(t,A,w,i,phi)
diminish = 0.001;
t_d = t(1):diminish:t(length(t));
print(t_d)
z = novel_wave(t_d,A,w,i,phi);
disp(z)
out_d = zeros(length(t_d));
for i = 1:1:length(t_d)-1
    out_d(i) = (z(i+1) - z(i))/diminish;
end
st = (t(length(t)) - t(1))/(length(t)-1);
out = out_d;
